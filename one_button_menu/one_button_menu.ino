void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);
  
  if(!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); // Don't proceed, loop forever
  }

  button.attachClick(click);
  button.attachDoubleClick(doubleclick);
  button.attachLongPressStart(longPressStart);

  display.clearDisplay();
  display.setTextSize(2); // Draw 2X-scale text
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(10, 0);
  display.println(F("long press to menu"));
  display.display();
}

void loop() {
  // put your main code here, to run repeatedly:
  button.tick();
  
  
}


void go_to_menu(){
  display.clearDisplay();
  //display.drawBitmap(0,1,two_after_2,128,32,WHITE);
  display.drawBitmap(0,1,zero_pix,128,32,WHITE);
  display.drawBitmap(40,1,zero_pix2,128,32,WHITE);
  display.drawBitmap(80,1,two_before,128,32,WHITE);
  display.display();
  
}

void click(){
  lastmillis = millis();
  pic++;
  
  if(pic>maxPics-1){
    pic=0;
    display.clearDisplay();
    display.drawBitmap(0,1,zero_after,128,32,WHITE);
    display.drawBitmap(40,1,one_before,128,32,WHITE);
    display.drawBitmap(80,1,two_before,128,32,WHITE);
    display.display();
    
  }
  else{
    
    if(pic==0){
      display.clearDisplay();
      display.drawBitmap(0,1,zero_after,128,32,WHITE);
      display.drawBitmap(40,1,one_before,128,32,WHITE);
      display.drawBitmap(80,1,two_before,128,32,WHITE);
      display.display();
    }
    else if(pic==1){
      display.clearDisplay();
      display.drawBitmap(0,1,zero_before,128,32,WHITE);
      display.drawBitmap(40,1,one_after,128,32,WHITE);
      display.drawBitmap(80,1,two_before,128,32,WHITE);
      display.display();
    }
    else if(pic==2){
      display.clearDisplay();
      display.drawBitmap(0,1,zero_before,128,32,WHITE);
      display.drawBitmap(40,1,one_before,128,32,WHITE);
      display.drawBitmap(80,1,two_after,128,32,WHITE);
      display.display();
    }
    
  }
  Serial.println(pic);
  
}

void doubleclick(){
  lastmillis = millis();
  if(pic==0){
    display.clearDisplay();
    display.drawBitmap(0,1,zero_after,128,32,WHITE);
    display.display();
  }
  else if (pic==1){
    display.clearDisplay();
    display.drawBitmap(0,1,one_after,128,32,WHITE);
    display.display();
  }
  else if (pic==2){
    display.clearDisplay();
    display.drawBitmap(0,1,two_after,128,32,WHITE);
    display.display();
  }
  
}

void longPressStart(){
  lastmillis = millis();
  pic=-1;
  go_to_menu();
}
